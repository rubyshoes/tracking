%html
  %head
    %title= full_title(yield(:title))
    = render 'layouts/stylesheets'
    = csrf_meta_tag
  %body
    #page.round
      #page2.round
        = render 'layouts/header'
        %section.round
          - flash.each do |key, value|
            = content_tag(:div, value, :class => "flash #{key}")
          = yield
        = render 'layouts/footer'
